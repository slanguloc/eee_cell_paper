	Doi = {10.1523/JNEUROSCI.4759-11.2012},
	Eprint = {http://www.jneurosci.org/content/32/14/4992.full.pdf+html},
	Group = {Brain Anatomy and Physiology - Motor cortex},
	Journal = {The Journal of Neuroscience},
	Number = {14},
	Pages = {4992-5001},
	Read = {1},
	Title = {Hierarchical Connectivity and Connection-Specific Dynamics in the Corticospinal--Corticostriatal Microcircuit in Mouse Motor Cortex},
