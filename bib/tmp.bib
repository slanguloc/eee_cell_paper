	Number = {157},
	Title = {Multitarget multiscale simulation for pharmacological treatment of dystonia in motor cortex},
	Url = {http://www.frontiersin.org/experimental_pharmacology_and_drug_discovery/10.3389/fphar.2016.00157/abstract},
