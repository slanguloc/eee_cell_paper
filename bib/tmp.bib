	Issn = {0022-3077},
	Journal = {Journal of Neurophysiology},
	Number = {4},
	Pages = {1847--1866},
	Publisher = {American Physiological Society},
	Title = {Dendritic Excitability and Neuronal Morphology as Determinants of Synaptic Efficacy},
