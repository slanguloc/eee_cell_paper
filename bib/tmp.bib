	Publisher = {Public Library of Science},
	Title = {Training Excitatory-Inhibitory Recurrent Neural Networks for Cognitive Tasks: A Simple and Flexible Framework},
	Url = {http://dx.doi.org/10.1371%2Fjournal.pcbi.1004792},
	Volume = {12},
	Year = {2016},
