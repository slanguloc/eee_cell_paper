	Doi = {10.3389/neuro.11.010.2009},
	Issn = {1662-5196},
	Journal = {Frontiers in Neuroinformatics},
	Number = {10},
	Title = {A component-based extension framework for large-scale parallel simulations in NEURON},
	Url = {http://www.frontiersin.org/neuroinformatics/10.3389/neuro.11.010.2009/abstract},
