	Eprint = {http://cercor.oxfordjournals.org/content/early/2016/10/20/cercor.bhw237.full.pdf+html},
	Journal = {Cerebral Cortex},
	Title = {Hybrid Scheme for Modeling Local Field Potentials from Point-Neuron Networks},
	Url = {http://cercor.oxfordjournals.org/content/early/2016/10/20/cercor.bhw237.abstract},
