	Issn = {1662-453X},
	Journal = {Frontiers in Neuroscience},
	Number = {28},
	Title = {Restoring behavior via inverse neurocontroller in a lesioned cortical spiking model driving a virtual arm},
	Url = {http://www.frontiersin.org/neuroprosthetics/10.3389/fnins.2016.00028/abstract},
