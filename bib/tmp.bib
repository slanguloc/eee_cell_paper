	Eprint = {http://www.sciencemag.org/content/345/6204/1616.full.pdf},
	Journal = {Science},
	Number = {6204},
	Pages = {1616-1620},
	Title = {A critical time window for dopamine actions on the structural plasticity of dendritic spines},
