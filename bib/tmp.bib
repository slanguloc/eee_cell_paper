	Issn = {1662-5102},
	Journal = {Frontiers in Cellular Neuroscience},
	Pages = {233},
	Title = {Anatomy and physiology of the thick-tufted layer 5 pyramidal neuron},
	Url = {http://journal.frontiersin.org/article/10.3389/fncel.2015.00233},
