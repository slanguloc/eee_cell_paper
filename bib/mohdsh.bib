  keywords    = {Adult; Blood Pressure, drug effects; Cognition, drug effects; Dissociative Disorders, chemically induced/diagnosis/physiopathology; Dose-Response Relationship, Drug; Double-Blind Method; Female; Frontal Lobe, drug effects/physiology; Homovanillic Acid, blood; Humans; Hydrocortisone, blood; Ketamine, pharmacology; Male; Methoxyhydroxyphenylglycol, blood; Neuropsychological Tests; Perception, drug effects; Prolactin, blood; Psychiatric Status Rating Scales; Psychoses, Substance-Induced, diagnosis/etiology/physiopathology; Pulse, drug effects; Receptors, N-Methyl-D-Aspartate, drug effects/physiology; Schizophrenia, chemically induced/diagnosis/physiopathology; Schizophrenic Psychology},
  medline-pst = {ppublish},
