  title       = {Measuring multiple spike train synchrony.},
  year        = {2009},
  language    = {eng},
  volume      = {183},
  number      = {2},
  month       = {Oct},
  pages       = {287--299},
  doi         = {10.1016/j.jneumeth.2009.06.039},
  url         = {http://dx.doi.org/10.1016/j.jneumeth.2009.06.039},
  abstract    = {Measures of multiple spike train synchrony are essential in order to study issues such as spike timing reliability, network synchronization, and neuronal coding. These measures can broadly be divided in multivariate measures and averages over bivariate measures. One of the most recent bivariate approaches, the ISI-distance, employs the ratio of instantaneous interspike intervals (ISIs). In this study we propose two extensions of the ISI-distance, the straightforward averaged bivariate ISI-distance and the multivariate ISI-diversity based on the coefficient of variation. Like the original measure these extensions combine many properties desirable in applications to real data. In particular, they are parameter-free, time scale independent, and easy to visualize in a time-resolved manner, as we illustrate with in vitro recordings from a cortical neuron. Using a simulated network of Hindemarsh-Rose neurons as a controlled configuration we compare the performance of our methods in distinguishing different levels of multi-neuron spike train synchrony to the performance of six other previously published measures. We show and explain why the averaged bivariate measures perform better than the multivariate ones and why the multivariate ISI-diversity is the best performer among the multivariate methods. Finally, in a comparison against standard methods that rely on moving window estimates, we use single-unit monkey data to demonstrate the advantages of the instantaneous nature of our methods.},
  file        = {:Kreuz2009Measuring.pdf:PDF;Kreuz2009Measuring.pdf:Kreuz2009Measuring.pdf:PDF},
  institution = {Institute for Nonlinear Sciences, University of California, San Diego, CA, USA. tkreuz@ucsd.edu},
  journal     = {J Neurosci Methods},
  keywords    = {Action Potentials, physiology; Animals; Computer Simulation; Electric Stimulation, methods; Nerve Net, physiology; Neural Inhibition; Neural Networks (Computer); Neurons, physiology; Oscillometry, methods; Patch-Clamp Techniques; Rats; Rats, Long-Evans; Reaction Time, physiology; Time Factors},
  medline-pst = {ppublish},
  owner       = {mohdsh},
  pii         = {S0165-0270(09)00361-6},
  pmid        = {19591867},
  timestamp   = {2016.04.01},
}

@Article{Kreuz2011Time,
  author      = {Kreuz, Thomas and Chicharro, Daniel and Greschner, Martin and Andrzejak, Ralph G.},
  title       = {Time-resolved and time-scale adaptive measures of spike train synchrony.},
  year        = {2011},
  language    = {eng},
  volume      = {195},
  number      = {1},
  month       = {Jan},
  pages       = {92--106},
  doi         = {10.1016/j.jneumeth.2010.11.020},
  url         = {http://dx.doi.org/10.1016/j.jneumeth.2010.11.020},
  abstract    = {A wide variety of approaches to estimate the degree of synchrony between two or more spike trains have been proposed. One of the most recent methods is the ISI-distance which extracts information from the interspike intervals (ISIs) by evaluating the ratio of the instantaneous firing rates. In contrast to most previously proposed measures it is parameter free and time-scale independent. However, it is not well suited to track changes in synchrony that are based on spike coincidences. Here we propose the SPIKE-distance, a complementary measure which is sensitive to spike coincidences but still shares the fundamental advantages of the ISI-distance. In particular, it is easy to visualize in a time-resolved manner and can be extended to a method that is also applicable to larger sets of spike trains. We show the merit of the SPIKE-distance using both simulated and real data.},
  file        = {:Kreuz2011Time.pdf:PDF;Kreuz2011Time.pdf:Kreuz2011Time.pdf:PDF},
  institution = {Institute for Complex Systems, Consiglio Nazionale delle Ricerche, Sesto Fiorentino, Italy. thomas.kreuz@cnr.it},
  journal     = {J Neurosci Methods},
  keywords    = {Action Potentials, physiology; Animals; Humans; Models, Neurological; Neural Conduction, physiology; Retinal Ganglion Cells, physiology; Time},
  medline-pst = {ppublish},
  owner       = {mohdsh},
  pii         = {S0165-0270(10)00656-4},
  pmid        = {21129402},
  timestamp   = {2016.04.01},
}

@Article{Kreuz2013Monitoring,
  author      = {Kreuz, Thomas and Chicharro, Daniel and Houghton, Conor and Andrzejak, Ralph G. and Mormann, Florian},
  title       = {Monitoring spike train synchrony.},
  year        = {2013},
  language    = {eng},
  volume      = {109},
  number      = {5},
  month       = {Mar},
  pages       = {1457--1472},
  doi         = {10.1152/jn.00873.2012},
  url         = {http://dx.doi.org/10.1152/jn.00873.2012},
  abstract    = {Recently, the SPIKE-distance has been proposed as a parameter-free and timescale-independent measure of spike train synchrony. This measure is time resolved since it relies on instantaneous estimates of spike train dissimilarity. However, its original definition led to spuriously high instantaneous values for eventlike firing patterns. Here we present a substantial improvement of this measure that eliminates this shortcoming. The reliability gained allows us to track changes in instantaneous clustering, i.e., time-localized patterns of (dis)similarity among multiple spike trains. Additional new features include selective and triggered temporal averaging as well as the instantaneous comparison of spike train groups. In a second step, a causal SPIKE-distance is defined such that the instantaneous values of dissimilarity rely on past information only so that time-resolved spike train synchrony can be estimated in real time. We demonstrate that these methods are capable of extracting valuable information from field data by monitoring the synchrony between neuronal spike trains during an epileptic seizure. Finally, the applicability of both the regular and the real-time SPIKE-distance to continuous data is illustrated on model electroencephalographic (EEG) recordings.},
  file        = {:Kreuz2013Monitoring.pdf:PDF;Kreuz2013Monitoring.pdf:Kreuz2013Monitoring.pdf:PDF},
  institution = {Institute for Complex Systems, CNR, Sesto Fiorentino, Italy. thomas.kreuz@cnr.it},
  journal     = {J Neurophysiol},
  keywords    = {Action Potentials, physiology; Animals; Brain, physiopathology; Electroencephalography Phase Synchronization; Electrophysiology, methods; Epilepsy, physiopathology; Humans; Seizures, physiopathology; Time Factors},
  medline-pst = {ppublish},
