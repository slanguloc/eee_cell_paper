}

@Article{Chadderdon2014Motor,
  author      = {George L Chadderdon and Ashutosh Mohan and Benjamin A Suter and Samuel A Neymotin and Cliff C Kerr and Joseph T Francis and Gordon M G Shepherd and William W Lytton},
  title       = {Motor cortex microcircuit simulation based on brain activity mapping.},
  year        = {2014},
