@Article{Arnsten2010Dynamic,
  author      = {Amy F T Arnsten and Constantinos D Paspalas and Nao J Gamo and Yang Yang and Min Wang},
  title       = {Dynamic Network Connectivity: A new form of neuroplasticity.},
  year        = {2010},
  language    = {eng},
  volume      = {14},
  number      = {8},
  month       = {Aug},
  pages       = {365--375},
