  groups      = {ADHD_EEG},
  institution = {Department of Psychiatry and Psychotherapy, Ludwig-Maximilians-University Munich, Nussbaumstrasse 7, 80336 Munich, Germany. Susanne.Karch@med.uni-muenchen.de},
  journal     = {J Psychiatr Res},
  keywords    = {Adolescent; Adult; Attention Deficit Disorder with Hyperactivity, physiopathology; Brain Waves, physiology; Electroencephalography, methods; Executive Function, physiology; Female; Humans; Inhibition (Psychology); Male; Middle Aged; Neuropsychological Tests; Up-Regulation, physiology; Wechsler Scales; Young Adult},
  medline-pst = {ppublish},
