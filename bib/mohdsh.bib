  keywords    = {Adolescent; Adult; Association Learning, physiology; Brain Mapping; Dentate Gyrus, physiology; Female; Functional Neuroimaging; Hippocampus, physiology; Humans; Magnetic Resonance Imaging; Male; Memory, Episodic; Parahippocampal Gyrus, physiology; Pattern Recognition, Visual, physiology; Space Perception, physiology; Temporal Lobe, physiology; Time Factors; Young Adult},
  medline-pst = {ppublish},
