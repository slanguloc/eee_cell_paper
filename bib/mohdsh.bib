@Article{Pittman-Polletta2015Brain,
  author      = {Pittman-Polletta, Benjamin R. and Kocsis, Bernat and Vijayan, Sujith and Whittington, Miles A. and Kopell, Nancy J.},
  title       = {Brain rhythms connect impaired inhibition to altered cognition in schizophrenia.},
