@Article{liu1999variability,
  author    = {Liu, G and Choi, S and Tsien, R W},
  title     = {Variability of neurotransmitter concentration and nonsaturation of postsynaptic {AMPA} receptors at synapses in hippocampal cultures and slices},
  year      = {1999},
  volume    = {22},
  number    = {2},
  month     = feb,
  pages     = {395--409},
  note      = {{PMID:} 10069344},
  issn      = {0896-6273},
  url       = {http://www.ncbi.nlm.nih.gov/pubmed/10069344},
  abstract  = {To understand the elementary unit of synaptic communication between {CNS} neurons, one must know what causes the variability of quantal postsynaptic currents and whether unitary packets of transmitter saturate postsynaptic receptors. We studied single excitatory synapses between hippocampal neurons in culture. Focal glutamate application at individual postsynaptic sites evoked currents {(I(glu))} with little variability compared with quantal excitatory postsynaptic currents {(EPSCs).} The maximal I(glu) was {\textgreater}2-fold larger than the median {EPSC.} Thus, variations in [glu]cleft are the main source of variability in {EPSC} size, and glutamate receptors are generally far from saturation during quantal transmission. This conclusion was verified by molecular antagonism experiments in hippocampal cultures and slices. The general lack of glutamate receptor saturation leaves room for increases in [glu]cleft as a mechanism for synaptic plasticity.},
  journal   = {Neuron},
  keywords  = {Animals, Cells, Cultured, Excitatory Postsynaptic Potentials, Glutamic Acid, Hippocampus, Neurotransmitter Agents, Osmolar Concentration, Presynaptic Terminals, Rats, Receptor Aggregation, Receptors, {AMPA}, Receptors, Glutamate, Receptors, Neurotransmitter, Stimulation, Chemical, Synapses, Synaptic Transmission},
  timestamp = {2016.04.01},
}

@Article{Llinas2005Rhythmic,
  author      = {Llin{\'{a}}s, Rodolfo and Urbano, Francisco J. and Leznik, Elena and Ram{\'{\i}}rez, Rey R. and {van Marle}, Hein J F.},
