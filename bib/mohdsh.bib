@Article{Boergers2005Background,
  author      = {B{\"{o}}rgers, Christoph and Epstein, Steven and Kopell, Nancy J},
  title       = {Background gamma rhythmicity and attention in cortical local circuits: a computational study.},
  year        = {2005},
  language    = {eng},
  volume      = {102},
  number      = {19},
  month       = {May},
  pages       = {7002--7007},
