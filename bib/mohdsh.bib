  journal     = {Proc Natl Acad Sci U S A},
  keywords    = {Action Potentials, drug effects/physiology; Animals; Beta Rhythm, drug effects/physiology; Carbachol, administration /&/ dosage; Cholinergic Agonists, administration /&/ dosage; Dopamine, physiology; Humans; Mice; Mice, Inbred C57BL; Models, Neurological; Nerve Net, physiopathology; Neurons, drug effects/physiology; Parkinsonian Disorders, physiopathology; Visual Cortex, drug effects/physiopathology; gamma-Aminobutyric Acid, physiology},
  medline-pst = {ppublish},
