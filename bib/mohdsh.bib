@Article{grebenyuk2004post-synaptic,
  author     = {Grebenyuk, Sergei E and Lozovaya, Natasha A and Tsintsadze, Timur S and Krishtal, Oleg A},
  title      = {Post-synaptic N-methyl-d-aspartate signalling in hippocampal neurons of rat: spillover increases the impact of each spike in a short burst discharge},
  year       = {2004},
  volume     = {361},
  number     = {1-3},
  month      = may,
  pages      = {60–63},
  note       = {{PMID:} 15135893},
  issn       = {0304-3940},
  doi        = {10.1016/j.neulet.2004.02.040},
  url        = {http://www.ncbi.nlm.nih.gov/pubmed/15135893},
  abstract   = {High-frequency burst discharges in hippocampus typically consist of less than ten spikes fired at frequencies too high to be followed by a post-synaptic neuron. How significant are these numbers for synaptic signalling? We have measured the N-methyl-d-aspartate {(NMDA)} component of the excitatory post-synaptic current {(EPSC(NMDA))} in hippocampal {CA1} neurons of rat after burst discharge of variable duration. The synaptic facilitation is accompanied by a slow-down of the {EPSC(NMDA)} which develops on a spike-to-spike basis. Consequently the charge transferred by the after-burst {EPSC(NMDA)} is increased with each spike. The phenomenon is most probably due to the spillover-mediated recruitment of extrasynaptic {NMDA} receptors. In terms of post-synaptic signalling it dramatically increases the impact of each spike in a short burst discharge.},
  journal    = {Neuroscience Letters},
  keywords   = {Action Potentials, Action Potentials, Animals, Animals, Excitatory Amino Acid Antagonists, Excitatory Amino Acid Antagonists, Excitatory Postsynaptic Potentials, Excitatory Postsynaptic Potentials, Glutamic Acid, Glutamic Acid, Hippocampus, Hippocampus, Neural Pathways, Neural Pathways, Neurons, Neurons, {N-Methyl-D-Aspartate}, Rats, Rats, Rats, Wistar, Receptors, Receptors, {N-Methyl-D-Aspartate}, Signal Transduction, Signal Transduction, Synaptic Membranes, Synaptic Membranes, Synaptic Transmission, Synaptic Transmission, Wistar},
  shorttitle = {Post-synaptic N-methyl-d-aspartate signalling in hippocampal neurons of rat},
  timestamp  = {2016.04.01},
}

@Article{Grienberger2012Imaging,
  author      = {Christine Grienberger and Arthur Konnerth},
  title       = {Imaging calcium in neurons.},
  year        = {2012},
  language    = {eng},
  volume      = {73},
  number      = {5},
