@Article{Tronson2012Fear,
  author      = {Tronson, Natalie C. and Corcoran, Kevin A. and Jovasevic, Vladimir and Radulovic, Jelena},
  title       = {Fear conditioning and extinction: emotional states encoded by distinct signaling pathways.},
  year        = {2012},
  language    = {eng},
  volume      = {35},
