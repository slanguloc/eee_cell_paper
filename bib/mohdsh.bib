@Article{Reynolds1990Deficit,
  author      = {Reynolds, G. P. and Czudek, C. and Andrews, H. B.},
  title       = {Deficit and hemispheric asymmetry of GABA uptake sites in the hippocampus in schizophrenia.},
  year        = {1990},
