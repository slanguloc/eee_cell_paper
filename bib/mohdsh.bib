}

@Article{Zuurman2009Pharmacodynamic,
  author      = {Zuurman, L. and Passier, P C C M. and {de Kam}, Ml and Kleijn, H. J. and Cohen, A. F. and {van Gerven}, J M A.},
  title       = {Pharmacodynamic and pharmacokinetic effects of the intravenously administered CB1 receptor agonist Org 28611 in healthy male volunteers.},
