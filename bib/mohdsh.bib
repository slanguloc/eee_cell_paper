@Article{Zheng2016Spatial,
  author      = {Zheng, Chenguang and Bieri, Kevin Wood and Hsiao, Yi-Tse and Colgin, Laura Lee},
  title       = {Spatial Sequence Coding Differs during Slow and Fast Gamma Rhythms in the Hippocampus.},
  year        = {2016},
  language    = {eng},
  volume      = {89},
  number      = {2},
  month       = {Jan},
  pages       = {398--408},
  doi         = {10.1016/j.neuron.2015.12.005},
  url         = {http://dx.doi.org/10.1016/j.neuron.2015.12.005},
  abstract    = {Spatiotemporal trajectories are coded by "theta sequences," ordered series of hippocampal place cell spikes that reflect the order of behavioral experiences. Theta sequences are thought to be organized by co-occurring gamma rhythms (∼25-100 Hz). However, how sequences of locations are represented during distinct slow (∼25-55 Hz) and fast (∼60-100 Hz) gamma subtypes remains poorly understood. We found that slow gamma-associated theta sequences activated on a compressed timescale and represented relatively long paths extending ahead of the current location. Fast gamma-associated theta sequences more closely followed an animal's actual location in real time. When slow gamma occurred, sequences of locations were represented across successive slow gamma phases. Conversely, fast gamma phase coding of spatial sequences was not observed. These findings suggest that slow gamma promotes activation of temporally compressed representations of upcoming trajectories, whereas fast gamma supports coding of ongoing trajectories in real time.},
  file        = {Zheng2016Spatial.pdf:Zheng2016Spatial.pdf:PDF},
  groups      = {gamma_hpc},
  institution = {Center for Learning and Memory, University of Texas at Austin, Austin, TX 78712-0805, USA; Institute for Neuroscience, University of Texas at Austin, Austin, TX 78712-0805, USA; Department of Neuroscience, University of Texas at Austin, Austin, TX 78712-0805, USA. Electronic address: colgin@mail.clm.utexas.edu.},
  journal     = {Neuron},
  keywords    = {Action Potentials, physiology; Animals; Gamma Rhythm, physiology; Hippocampus, physiology; Male; Rats; Rats, Long-Evans; Theta Rhythm, physiology},
  medline-pst = {ppublish},
  owner       = {mohdsh},
  pii         = {S0896-6273(15)01076-4},
  pmc         = {PMC4731025},
  pmid        = {26774162},
  timestamp   = {2016.08.24},
}

@Article{Zheng2015relationship,
  author      = {Zheng, Chenguang and Bieri, Kevin Wood and Trettel, Sean Gregory and Colgin, Laura Lee},
  title       = {The relationship between gamma frequency and running speed differs for slow and fast gamma rhythms in freely behaving rats.},
  year        = {2015},
  language    = {eng},
  volume      = {25},
  number      = {8},
  month       = {Aug},
  pages       = {924--938},
  doi         = {10.1002/hipo.22415},
  url         = {http://dx.doi.org/10.1002/hipo.22415},
  abstract    = {In hippocampal area CA1 of rats, the frequency of gamma activity has been shown to increase with running speed (Ahmed and Mehta, 2012). This finding suggests that different gamma frequencies simply allow for different timings of transitions across cell assemblies at varying running speeds, rather than serving unique functions. However, accumulating evidence supports the conclusion that slow (∼25-55 Hz) and fast (∼60-100 Hz) gamma are distinct network states with different functions. If slow and fast gamma constitute distinct network states, then it is possible that slow and fast gamma frequencies are differentially affected by running speed. In this study, we tested this hypothesis and found that slow and fast gamma frequencies change differently as a function of running speed in hippocampal areas CA1 and CA3, and in the superficial layers of the medial entorhinal cortex (MEC). Fast gamma frequencies increased with increasing running speed in all three areas. Slow gamma frequencies changed significantly less across different speeds. Furthermore, at high running speeds, CA3 firing rates were low, and MEC firing rates were high, suggesting that CA1 transitions from CA3 inputs to MEC inputs as running speed increases. These results support the hypothesis that slow and fast gamma reflect functionally distinct states in the hippocampal network, with fast gamma driven by MEC at high running speeds and slow gamma driven by CA3 at low running speeds.},
  file        = {Zheng2015relationship.pdf:Zheng2015relationship.pdf:PDF},
  groups      = {gamma_hpc},
  institution = {Department of Neuroscience, University of Texas, Austin, Texas.},
  journal     = {Hippocampus},
  keywords    = {Action Potentials, physiology; Animals; Electrodes, Implanted; Gamma Rhythm, physiology; Hippocampus, physiology; Male; Rats; Rats, Long-Evans; Running, physiology; Spectrum Analysis; Statistics as Topic; Time Factors; Wakefulness, physiology},
  medline-pst = {ppublish},
  owner       = {mohdsh},
  pmc         = {PMC4499477},
  pmid        = {25601003},
  timestamp   = {2016.08.24},
}

