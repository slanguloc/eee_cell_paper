  journal     = {Nat Rev Neurosci},
  keywords    = {Animals; Biological Clocks, physiology; Cortical Synchronization, psychology; Humans; Nerve Net, physiology; Neurons, physiology; Schizophrenia, physiopathology},
  medline-pst = {ppublish},
  owner       = {mohdsh},
  pii         = {nrn2774},
  pmid        = {20087360},
  timestamp   = {2016.04.01},
}

@Article{Uhlhaas2013High,
  author      = {Uhlhaas, Peter J. and Singer, Wolf},
  title       = {High-frequency oscillations and the neurobiology of schizophrenia.},
  year        = {2013},
  language    = {eng},
  volume      = {15},
  number      = {3},
  month       = {Sep},
  pages       = {301--313},
  abstract    = {Neural oscillations at low- and high-frequency ranges are a fundamental feature of large-scale networks. Recent evidence has indicated that schizophrenia is associated with abnormal amplitude and synchrony of oscillatory activity, in particular, at high (beta/gamma) frequencies. These abnormalities are observed during task-related and spontaneous neuronal activity which may be important for understanding the pathophysiology of the syndrome. In this paper, we shall review the current evidence for impaired beta/gamma-band oscillations and their involvement in cognitive functions and certain symptoms of the disorder. In the first part, we will provide an update on neural oscillations during normal brain functions and discuss underlying mechanisms. This will be followed by a review of studies that have examined high-frequency oscillatory activity in schizophrenia and discuss evidence that relates abnormalities of oscillatory activity to disturbed excitatory/inhibitory (E/I) balance. Finally, we shall identify critical issues for future research in this area.Abstract available from the publisher.Abstract available from the publisher.},
  file        = {Uhlhaas2013High.pdf:Uhlhaas2013High.pdf:PDF;Uhlhaas2013High.pdf:Uhlhaas2013High.pdf:PDF;Uhlhaas2013High.pdf:Uhlhaas2013High.pdf:PDF},
  institution = {Department of Neurophysiology, Max Planck Institute for Brain Research, Deutschorclenstr. 46, Frankfurt am Main, 60528, Germany; Ernst Strüngmann Institute (ESI) for Neuroscience, in Cooperation with Max Planck Society, Deutschorclenstr. 46, Frankfurt am Main, 60528, Germany; Institute of Neuroscience and Psychology, University of Glasgow, 58 Hillheacl Street, Glasgow G12 8QB, UK.},
  journal     = {Dialogues Clin Neurosci},
  keywords    = {Animals; Brain Waves, physiology; Brain, pathology/physiology; Electroencephalography, methods; Humans; Nerve Net, physiology; Periodicity; Schizophrenia, diagnosis/physiopathology},
  medline-pst = {ppublish},
