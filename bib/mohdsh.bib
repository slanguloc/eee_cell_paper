  pii         = {ejn1407},
  pmid        = {11168550},
  timestamp   = {2016.04.01},
}

@Article{Nyiri2005GABAB,
  author      = {Ny{\'{i}}ri, G{\'{a}}bor and Szabadits, Eszter and Cser{\'{e}}p, Csaba and Mackie, Ken and Shigemoto, Ryuichi and Freund, Tam{\'{a}}s F.},
  title       = {GABAB and CB1 cannabinoid receptor expression identifies two types of septal cholinergic neurons.},
  year        = {2005},
  language    = {eng},
  volume      = {21},
  number      = {11},
  month       = {Jun},
  pages       = {3034--3042},
  doi         = {10.1111/j.1460-9568.2005.04146.x},
  url         = {http://dx.doi.org/10.1111/j.1460-9568.2005.04146.x},
  abstract    = {Septohippocampal cholinergic neurons play key roles in learning and memory processes, and in the generation of hippocampal theta rhythm. The range of receptors for endogenous modulators expressed on these neurons is unclear. Here we describe GABA(B) 1a/b receptor (GABA(B)R) and type 1 cannabinoid receptor (CB(1)R) expression in rat septal cholinergic [i.e. choline acetyltransferase (ChAT)-positive] cells. Using double immunofluorescent staining, we found that almost two-thirds of the cholinergic cells in the rat medial septum were GABA(B)R positive, and that these cells had significantly larger somata than did GABA(B)R-negative cholinergic neurons. We detected CB(1)R labelling in somata after axonal protein transport was blocked by colchicine. In these animals about one-third of the cholinergic cells were CB(1)R positive. These cells again had larger somata than CB(1)R-negative cholinergic neurons. The analyses confirmed that the size of GABA(B)R-positive and CB(1)R-positive cholinergic cells were alike, and all CB(1)R-positive cholinergic cells were GABA(B)R positive as well. CB(1)R-positive cells were invariably ChAT positive. All retrogradely labelled septohippocampal cholinergic cells were positive for GABA(B)R and at least half of them also for CB(1)R. These data shed light on the existence of at least two cholinergic cell types in the medial septum: one expresses GABA(B)R and CB(1)R, has large somata and projects to the hippocampus, whereas the other is negative for GABA(B)R and CB(1)R and has smaller somata. The results also suggest that cholinergic transmission in the hippocampus is fine-tuned by endocannabinoid signalling.},
  file        = {Nyiri2005GABAB.pdf:Nyiri2005GABAB.pdf:PDF;Nyiri2005GABAB.pdf:Nyiri2005GABAB.pdf:PDF},
  institution = {Institute of Experimental Medicine, Hungarian Academy of Sciences, Budapest, 1083, Szigony u. 43., Hungary. nyiri@koki.hu},
  journal     = {Eur J Neurosci},
  keywords    = {Acetylcholine, metabolism; Animals; Cannabinoid Receptor Modulators, metabolism; Cell Count; Cell Size; Choline O-Acetyltransferase, metabolism; Cholinergic Fibers, classification/metabolism; Hippocampus, metabolism; Immunohistochemistry; Male; Neural Inhibition, physiology; Neural Pathways, metabolism; Neurons, classification/metabolism; Rats; Rats, Wistar; Receptor, Cannabinoid, CB1, metabolism; Receptors, GABA-B, metabolism; Septal Nuclei, metabolism; Synaptic Transmission, physiology; Theta Rhythm; gamma-Aminobutyric Acid, metabolism},
  medline-pst = {ppublish},
