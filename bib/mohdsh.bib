  journal     = {Neurosci Behav Physiol},
  keywords    = {Animals; Attention, physiology; Cortical Synchronization; Dominance, Cerebral, physiology; Hippocampus, physiology; Humans; Memory, physiology; Models, Neurological; Nerve Net, physiology; Neural Inhibition, physiology; Neural Networks (Computer)},
