@Article{Gloveli2005Orthogonal,
  author      = {Gloveli, Tengis and Dugladze, Tamar and Rotstein, Horacio G and Traub, Roger D and Monyer, Hannah and Heinemann, Uwe and Whittington, Miles A and Kopell, Nancy J},
  title       = {Orthogonal arrangement of rhythm-generating microcircuits in the hippocampus.},
