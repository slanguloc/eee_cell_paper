  institution = {Department of Psychiatry, Inje University Ilsan Paik Hospital, Goyang, Korea.},
  journal     = {Clin Psychopharmacol Neurosci},
  medline-pst = {ppublish},
  owner       = {mohdsh},
  pii         = {cpn.2015.13.2.180},
  pmc         = {PMC4540038},
