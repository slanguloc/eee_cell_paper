  keywords    = {Animals; Cocaine, pharmacology; Cognition, drug effects; Data Interpretation, Statistical; Dopamine Uptake Inhibitors, pharmacology; Dopamine, physiology; Electrodes, Implanted; Electrophysiological Phenomena, physiology; Executive Function, drug effects/physiology; Macaca mulatta; Nerve Net, drug effects/physiology; Prefrontal Cortex, drug effects/physiology; Psychomotor Performance, drug effects/physiology},
  medline-pst = {ppublish},
