@Article{Park2006Plasticity,
  author      = {Park, Mikyoung and Salgado, Jennifer M. and Ostroff, Linnaea and Helton, Thomas D. and Robinson, Camenzind G. and Harris, Kristen M. and Ehlers, Michael D.},
  title       = {Plasticity-induced growth of dendritic spines by exocytic trafficking from recycling endosomes.},
  year        = {2006},
  language    = {eng},
  volume      = {52},
  number      = {5},
  month       = {Dec},
  pages       = {817--830},
  doi         = {10.1016/j.neuron.2006.09.040},
  url         = {http://dx.doi.org/10.1016/j.neuron.2006.09.040},
  abstract    = {Dendritic spines are micron-sized membrane protrusions receiving most excitatory synaptic inputs in the mammalian brain. Spines form and grow during long-term potentiation (LTP) of synaptic strength. However, the source of membrane for spine formation and enlargement is unknown. Here we report that membrane trafficking from recycling endosomes is required for the growth and maintenance of spines. Using live-cell imaging and serial section electron microscopy, we demonstrate that LTP-inducing stimuli promote the mobilization of recycling endosomes and vesicles into spines. Preventing recycling endosomal transport abolishes LTP-induced spine formation. Using a pH-sensitive recycling cargo, we show that exocytosis from recycling endosomes occurs locally in spines, is triggered by activation of synaptic NMDA receptors, and occurs concurrently with spine enlargement. Thus, recycling endosomes provide membrane for activity-dependent spine growth and remodeling, defining a novel membrane trafficking mechanism for spine morphological plasticity and providing a mechanistic link between structural and functional plasticity during LTP.},
  file        = {Park2006Plasticity.pdf:Park2006Plasticity.pdf:PDF},
  groups      = {slc9a9/NHE9},
  institution = {Department of Neurobiology, Duke University Medical Center, Durham, North Carolina 27710, USA.},
