@Article{Haller2007Correlated,
  author      = {Haller, J. and M{\'{a}}ty{\'{a}}s, F. and Soproni, K. and Varga, B. and Barsy, B. and N{\'{e}}meth, B. and Mikics, E. and Freund, T. F. and H{\'{a}}jos, N.},
  title       = {Correlated species differences in the effects of cannabinoid ligands on anxiety and on GABAergic and glutamatergic synaptic transmission.},
  year        = {2007},
  language    = {eng},
  volume      = {25},
  number      = {8},
  month       = {Apr},
  pages       = {2445--2456},
  doi         = {10.1111/j.1460-9568.2007.05476.x},
  url         = {http://dx.doi.org/10.1111/j.1460-9568.2007.05476.x},
  abstract    = {Cannabinoid ligands show therapeutic potential in a variety of disorders including anxiety. However, the anxiety-related effects of cannabinoids remain controversial as agonists show opposite effects in mice and rats. Here we compared the effects of the cannabinoid agonist WIN-55,212 and the CB1 antagonist AM-251 in CD1 mice and Wistar rats. Special attention was paid to antagonist-agonist interactions, which had not yet been studied in rats. In mice, WIN-55,212 decreased whereas AM-251 increased anxiety. The antagonist abolished the effects of the agonist. In contrast, WIN-55,212 increased anxiety in rats. Surprisingly, the antagonist potentiated this effect. Cannabinoids affect both GABAergic and glutamatergic functions, which play opposite roles in anxiety. We hypothesized that discrepant findings resulted from species differences in the relative responsiveness of the two transmitter systems to cannabinoids. We investigated this hypothesis by studying the effects of WIN-55,212 on evoked hippocampal inhibitory and excitatory postsynaptic currents (IPSCs and EPSCs). IPSCs were one order of magnitude more sensitive to WIN-55,212 in mice than in rats. In mice, IPSCs were more sensitive than EPSCs to WIN-55,212. This is the first study showing that the relative cannabinoid sensitivity of GABA and glutamate neurotransmission is species-dependent. Based on behavioural and electrophysiological findings, we hypothesize that WIN-55,212 reduced anxiety in mice by affecting GABA neurotransmission whereas it increased anxiety in rats via glutamatergic mechanisms. In rats, AM-251 potentiated this anxiogenic effect by inhibiting the anxiolytic GABAergic mechanism. We suggest that the anxiety-related effects of cannabinoids depend on the relative cannabinoid responsiveness of GABAergic and glutamatergic neurotransmission.},
  file        = {Haller2007Correlated.pdf:Haller2007Correlated.pdf:PDF;Haller2007Correlated.pdf:Haller2007Correlated.pdf:PDF;Haller2007Correlated.pdf:Haller2007Correlated.pdf:PDF},
  institution = {Institute of Experimental Medicine, Department of Behavioral Neurobiology, 1450 Budapest, PO Box 67, Hungary. haller@koki.hu},
  journal     = {Eur J Neurosci},
  keywords    = {Analgesics, pharmacology/therapeutic use; Animals; Anti-Anxiety Agents, pharmacology/therapeutic use; Anxiety, drug therapy; Benzoxazines, pharmacology/therapeutic use; Cannabinoids, agonists/antagonists /&/ inhibitors/pharmacology/therapeutic use; Chlordiazepoxide, pharmacology/therapeutic use; Excitatory Postsynaptic Potentials, physiology; Exploratory Behavior, drug effects; Glutamic Acid, metabolism; Hippocampus, cytology/drug effects; Inhibitory Postsynaptic Potentials, physiology; Ligands; Male; Mice; Morpholines, pharmacology/therapeutic use; Naphthalenes, pharmacology/therapeutic use; Patch-Clamp Techniques; Piperidines, pharmacology/therapeutic use; Pyrazoles, pharmacology/therapeutic use; Rats; Rats, Wistar; Receptor, Cannabinoid, CB1, metabolism; Synaptic Transmission, physiology; gamma-Aminobutyric Acid, metabolism},
  medline-pst = {ppublish},
  owner       = {mohdsh},
  pii         = {EJN5476},
  pmid        = {17445240},
  timestamp   = {2016.04.01},
}

