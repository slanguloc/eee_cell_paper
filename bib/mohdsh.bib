  timestamp   = {2016.04.01},
}

@Article{Jin2005Mechanism,
  author      = {Jin, Rongsheng and Clark, Suzanne and Weeks, Autumn M. and Dudman, Joshua T. and Gouaux, Eric and Partin, Kathryn M.},
