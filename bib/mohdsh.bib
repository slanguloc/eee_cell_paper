  pmid        = {22118930},
  timestamp   = {2016.07.25},
}

@Article{Maren2013contextual,
  author      = {Maren, Stephen and Phan, K. Luan and Liberzon, Israel},
  title       = {The contextual brain: implications for fear conditioning, extinction and psychopathology.},
