@Article{Cano-Colino2012computational,
  author      = {M. Cano-Colino and A. Compte},
  title       = {A computational model for spatial working memory deficits in schizophrenia.},
  year        = {2012},
  language    = {eng},
  volume      = {45 Suppl 1},
  month       = {May},
  pages       = {S49--S56},
  doi         = {10.1055/s-0032-1306314},
  url         = {http://dx.doi.org/10.1055/s-0032-1306314},
  abstract    = {Cognitive deficits in schizophrenia have been hypothesized to be caused by altered synaptic transmission in circuits of the prefrontal cortex. 2 main hypotheses have been put forward: reduced inhibition and hypofunctional NMDA receptors. Recently, Lee et al. (2008) found that spatial working memory deficits in schizophrenic patients include a disproportionately high incidence of high-confidence error responses. Here, we have studied what synaptic dysfunction can generate this specific behavioral deficit using a computational network model of spatial working memory. We developed quantitative behavioral readout from our network simulations, which reflected the qualitative properties of underlying neural dynamics. We then analyzed the behavioral effect of the GABAergic and glutamatergic hypotheses on our network simulations. We found that reduction in inhibitory transmission in the network caused a reduction in performance through an increase of high-confidence errors, as in the experimental data. In contrast, a concerted reduction in NMDA-receptor-dependent transmission reduced performance via increased low-confidence errors. Only when NMDA receptors were specifically depleted in interneurons did the behavioral read-out of our network mimic the behavioral results for schizophrenic patients. Thus, dynamics in our model network support a role of both global inhibition reduction and hypofunctional NMDA receptors in interneurons in generating the behavioral deficits of simple spatial working memory tasks in schizophrenia.},
  institution = {Institut d'Investigacions Biomèdiques August Pi i Sunyer (IDIBAPS), Barcelona, Spain.},
  journal     = {Pharmacopsychiatry},
  keywords    = {Algorithms; Animals; Computer Simulation; Cues; Electrophysiological Phenomena; Fixation, Ocular, physiology; Haplorhini; Interneurons, physiology; Ion Channel Gating, physiology; Memory Disorders, etiology/psychology; Memory, Short-Term, physiology; Models, Neurological; Receptors, AMPA, physiology; Receptors, N-Methyl-D-Aspartate, physiology; Schizophrenic Psychology; Space Perception, physiology},
  medline-pst = {ppublish},
  owner       = {mohdsh},
  pmid        = {22565235},
  timestamp   = {2016.04.01},
}

