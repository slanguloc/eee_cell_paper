  journal     = {Trends Neurosci},
  keywords    = {Animals; Hippocampus, physiology; Humans; Models, Neurological; Movement, physiology; Neocortex, physiology; Nerve Net, physiology; Neural Networks (Computer); Neural Pathways, physiology; Periodicity},
  medline-pst = {ppublish},
