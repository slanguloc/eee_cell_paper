@Article{Lafenetre2007endocannabinoid,
  author      = {Lafen{\^{e}}tre, Pauline and Chaouloff, Francis and Marsicano, Giovanni},
  title       = {The endocannabinoid system in the processing of anxiety and fear and how CB1 receptors may modulate fear extinction.},
