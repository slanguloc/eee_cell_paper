@Article{clinton2003altered,
  author    = {Clinton, Sarah M and Haroutunian, Vahram and Davis, Kenneth L and {Meador-Woodruff}, James H},
  title     = {Altered transcript expression of {NMDA} receptor-associated postsynaptic proteins in the thalamus of subjects with schizophrenia},
  year      = {2003},
  volume    = {160},
  number    = {6},
  month     = jun,
  pages     = {1100–1109},
  note      = {{PMID:} 12777268},
  issn      = {{0002-953X}},
  url       = {http://www.ncbi.nlm.nih.gov/pubmed/12777268},
  abstract  = {{OBJECTIVE} {NMDA} receptor dysfunction has been implicated in the pathophysiology of schizophrenia. The {NMDA} receptor is a multimeric ligand-gated ion channel, and the obligate {NR(1)} subunit is expressed as one of eight isoforms due to the alternative splicing of exons 5, 21, and 22. Alternative splicing of {NR(1)} subunits modulates receptor function by influencing the association of {NR(1)} with other {NMDA} receptor subunits and myriad intracellular molecules, such as the postsynaptic density family of proteins that target {NMDA} receptors to the synaptic membrane and couple it to numerous signal transduction enzymes. Recently, the authors reported that the {NMDA} receptor subunits {NR(1)} and {NR(2C)} are abnormally expressed in the thalamus in schizophrenia. They hypothesized that this reduction is associated with specific {NR(1)} isoforms and that {NMDA} receptor-related postsynaptic density proteins are abnormally expressed. {METHOD} Using in situ hybridization, the authors examined expression of the transcripts encoding {NR(1)} isoforms containing exons 5, 21, or 22, and the {NMDA} receptor-related postsynaptic density proteins {NF-L}, {PSD93}, {PSD95}, and {SAP102.} {RESULTS} Reduced {NR(1)} subunit transcript expression was restricted to exon 22-containing isoforms. Increased expression of the {NMDA} receptor-associated postsynaptic density proteins {NF-L}, {PSD95}, and {SAP102} was also detected in the thalamus of subjects with schizophrenia. {CONCLUSIONS} These data support the hypothesis of glutamatergic abnormalities in schizophrenia and suggest that glutamatergic dysfunction may occur not only at the level of receptor expression but also within intracellular pathways associated with glutamate receptor-associated signal transduction.},
  journal   = {The American Journal of Psychiatry},
  keywords  = {Aged, Aged, Exons, Exons, Female, Female, Gene Expression, Gene Expression, Genetic, Humans, Humans, In Situ Hybridization, In Situ Hybridization, Intracellular Signaling Peptides and Proteins, Intracellular Signaling Peptides and Proteins, Male, Male, Membrane Proteins, Membrane Proteins, Nerve Tissue Proteins, Nerve Tissue Proteins, Neurofilament Proteins, Neurofilament Proteins, Neuropeptides, Neuropeptides, {N-Methyl-D-Aspartate}, Nuclear Proteins, Nuclear Proteins, Protein Isoforms, Protein Isoforms, Receptors, Receptors, {N-Methyl-D-Aspartate}, Schizophrenia, Schizophrenia, Signal Transduction, Signal Transduction, Thalamus, Thalamus, Transcription, Transcription Factors, Transcription Factors, Transcription, Genetic},
  timestamp = {2016.04.01},
}

