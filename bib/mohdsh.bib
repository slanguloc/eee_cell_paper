  pii         = {S1388-2457(11)00052-6},
  pmid        = {21310652},
  timestamp   = {2016.07.27},
}

@Article{Saad2015Is,
  author      = {Saad, Jacqueline F. and Kohn, Michael R. and Clarke, Simon and Lagopoulos, Jim and Hermens, Daniel F.},
  title       = {Is the Theta/Beta EEG Marker for ADHD Inherently Flawed?},
  year        = {2015},
  language    = {eng},
  month       = {Mar},
  doi         = {10.1177/1087054715578270},
  url         = {http://dx.doi.org/10.1177/1087054715578270},
  abstract    = {Despite advances in our understanding of ADHD as a neurodevelopmental disorder, robust biomarkers are yet to be established in clinical practice. More than 40 years of electroencephalography (EEG)-based research has culminated in the recent Food and Drug Administration (FDA) approval of the theta/beta (EEG power) ratio (TBR) as a diagnostic marker of ADHD.This review article focuses on resting-state EEG power research in ADHD.Inconsistent findings in the literature and suggestions of reduced specificity threaten the utility of TBR as a biomarker of ADHD. The use of fixed EEG bands may be a significant limitation, particularly in youth, and alternative approaches are needed.We propose that a personalized theta-to-alpha cut point or "transition frequency" is a better frame of reference for the measurement of TBR. Such an approach is better placed to test maturational lag and cortical hypoarousal models of ADHD and may in turn have greater utility in supporting diagnosis.},
  file        = {Saad2015Is.pdf:Saad2015Is.pdf:PDF},
  groups      = {ADHD_EEG},
  institution = {University of Sydney, Camperdown, New South Wales, Australia.},
  journal     = {J Atten Disord},
  medline-pst = {aheadofprint},
  owner       = {mohdsh},
  pii         = {1087054715578270},
  pmid        = {25823742},
  timestamp   = {2016.07.27},
}

@Article{Lenartowicz2014Use,
  author      = {Lenartowicz, Agatha and Loo, Sandra K.},
  title       = {Use of EEG to diagnose ADHD.},
  year        = {2014},
  language    = {eng},
  volume      = {16},
  number      = {11},
  month       = {Nov},
  pages       = {498},
  doi         = {10.1007/s11920-014-0498-0},
  url         = {http://dx.doi.org/10.1007/s11920-014-0498-0},
  abstract    = {Electroencephalography (EEG) has, historically, played a focal role in the assessment of neural function in children with attention deficit hyperactivity disorder (ADHD). We review here the most recent developments in the utility of EEG in the diagnosis of ADHD, with emphasis on the most commonly used and emerging EEG metrics and their reliability in diagnostic classification. Considering the clinical heterogeneity of ADHD and the complexity of information available from the EEG signals, we suggest that considerable benefits are to be gained from multivariate analyses and a focus towards understanding of the neural generators of EEG. We conclude that while EEG cannot currently be used as a diagnostic tool, vast developments in analytical and technological tools in its domain anticipate future progress in its utility in the clinical setting.},
  file        = {Lenartowicz2014Use.pdf:Lenartowicz2014Use.pdf:PDF},
  groups      = {ADHD_EEG},
  institution = {Semel Institute for Neuroscience and Human Behavior, David Geffen School of Medicine, 760 Westwood Pl. Suite 17-369, Los Angeles, CA, 90095, USA, alenarto@ucla.edu.},
  journal     = {Curr Psychiatry Rep},
  keywords    = {Attention Deficit Disorder with Hyperactivity, diagnosis; Electroencephalography, methods/standards; Evoked Potentials, physiology; Humans},
  medline-pst = {ppublish},
