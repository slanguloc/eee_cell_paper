  author     = {Toro, Carla Tatiana and Hallak, Jaime E and Dunham, Jason S and Leite, João P and Sakamoto, Américo C and Guarnieri, Ricardo and Fong, Vivian and Deakin, John F W},
  title      = {The {NR1} {N-methyl-D-aspartate} subunit and brain-derived neurotrophic factor in temporal lobe epilepsy hippocampus: a comparison of patients with and without coexisting psychiatric symptoms},
  year       = {2007},
  volume     = {48},
  number     = {12},
  month      = dec,
  pages      = {2352--2356},
  note       = {{PMID:} 17919302},
  issn       = {0013-9580},
  doi        = {10.1111/j.1528-1167.2007.01194.x},
  url        = {http://www.ncbi.nlm.nih.gov/pubmed/17919302},
  abstract   = {{PURPOSE} The glutamate {N-methyl-D-aspartate} {(NMDA)} receptor and the neurotrophin brain-derived neurotrophic factor have been implicated in the pathophysiology of schizophrenia and depression. Since these psychiatric disorders are common in temporal lobe epilepsy {(TLE)}, a comparison of {TLE} patients with and without coexisting psychiatric symptoms may be useful to unravel pathophysiologic mechanisms for psychosis or depression. {METHODS} We used immunoautoradiography to assess the {NR1} {NMDA} receptor subunit and brain-derived neurotrophic factor in resected {TLE} hippocampus. {RESULTS} No changes relative to comparison controls were found for {TLE} patients with schizophrenia-like psychosis or depression. Increased {NR1} was found in the dentate molecular layer in the dysphoria group and unmedicated depressed patients. {CONCLUSIONS} An increase in {NR1} protein in the dentate molecular layer suggests an upregulation of {NMDA} receptors in granule cells in {TLE} patients with dysphoria and depression. This finding is compatible with the theory that increased {NMDA} receptor function is involved in the pathogenesis of depression and that antidepressants may act by opposing this mechanism.},
  journal    = {Epilepsia},
  keywords   = {Adult, Adult, Analysis of Variance, Analysis of Variance, Autoradiography, Autoradiography, Blotting, Blotting, Western, {Brain-Derived} Neurotrophic Factor, {Brain-Derived} Neurotrophic Factor, Comorbidity, Comorbidity, Dentate Gyrus, Dentate Gyrus, Depressive Disorder, Depressive Disorder, Major, Epilepsy, Epilepsy, Temporal Lobe, Female, Female, Hippocampus, Hippocampus, Humans, Humans, Immunohistochemistry, Immunohistochemistry, Major, Male, Male, Mental Disorders, Mental Disorders, {N-Methyl-D-Aspartate}, Receptors, Receptors, {N-Methyl-D-Aspartate}, Schizophrenia, Schizophrenia, Schizophrenic Psychology, Schizophrenic Psychology, Temporal Lobe, Western},
  shorttitle = {The {NR1} {N-methyl-D-aspartate} subunit and brain-derived neurotrophic factor in temporal lobe epilepsy hippocampus},
  timestamp  = {2016.04.01},
}

@Article{Tort2007formation,
  author      = {Adriano B L Tort and Horacio G Rotstein and Tamar Dugladze and Tengis Gloveli and Nancy J Kopell},
  title       = {On the formation of gamma-coherent cell assemblies by oriens lacunosum-moleculare interneurons in the hippocampus.},
  year        = {2007},
  language    = {eng},
  volume      = {104},
  number      = {33},
  month       = {Aug},
  pages       = {13490--13495},
  doi         = {10.1073/pnas.0705708104},
  url         = {http://dx.doi.org/10.1073/pnas.0705708104},
  abstract    = {Gamma frequency (30-80 Hz) network oscillations have been observed in the hippocampus during several behavioral paradigms in which they are often modulated by a theta frequency (4-12 Hz) oscillation. Interneurons of the hippocampus have been shown to be crucially involved in rhythms generation, and several subtypes with distinct anatomy and physiology have been described. In particular, the oriens lacunosum-moleculare (O-LM) interneurons were shown to synapse on distal apical dendrites of pyramidal cells and to spike preferentially at theta frequency, even in the presence of gamma-field oscillations. O-LM cells have also recently been shown to present higher axonal ramification in the longitudinal axis of the hippocampus. By using a hippocampal network model composed of pyramidal cells and two types of interneurons (O-LM and basket cells), we show here that the O-LM interneurons lead to gamma coherence between anatomically distinct cell modules. We thus propose that this could be a mechanism for coupling longitudinally distant cells excited by entorhinal cortex inputs into gamma-coherent assemblies.},
  file        = {Tort2007formation.pdf:Tort2007formation.pdf:PDF;Tort2007formation.pdf:Tort2007formation.pdf:PDF;Tort2007formation.pdf:Tort2007formation.pdf:PDF},
  groups      = {gamma_generation},
  institution = {Department of Mathematics and Center for Biodynamics, Boston University, Boston, MA 02215, USA. tort@bu.edu},
  journal     = {Proc Natl Acad Sci U S A},
  keywords    = {Animals; Hippocampus, cytology; Interneurons, cytology; Mice; Mice, Inbred C57BL; Synapses},
  medline-pst = {ppublish},
  owner       = {mohdsh},
  pii         = {0705708104},
