  institution = {Department of Psychiatry, Hospital Universitari Mútua Terrassa, Universitat de Barcelona (UB), Plaça Dr. Robert, 5. 08221, Terrassa, Barcelona, Spain. gemmasl@msn.com},
  journal     = {Schizophr Res},
  keywords    = {Adolescent; Adult; Analysis of Variance; Benzamides; Cannabis, metabolism; Corpus Striatum, radionuclide imaging; Dopamine Antagonists; Female; Humans; Male; Prospective Studies; Psychotic Disorders, metabolism/pathology/radionuclide imaging; Pyrrolidines; Receptors, Dopamine D2, metabolism; Statistics, Nonparametric; Tomography, Emission-Computed, Single-Photon; Young Adult},
  medline-pst = {ppublish},
