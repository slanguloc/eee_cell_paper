@Article{Lisman2010thalamo,
  author      = {Lisman, John E. and Pi, Hyun Jae and Zhang, Yuchun and Otmakhova, Nonna A.},
  title       = {A thalamo-hippocampal-ventral tegmental area loop may produce the positive feedback that underlies the psychotic break in schizophrenia.},
  year        = {2010},
  language    = {eng},
  volume      = {68},
  number      = {1},
  month       = {Jul},
  pages       = {17--24},
  doi         = {10.1016/j.biopsych.2010.04.007},
  url         = {http://dx.doi.org/10.1016/j.biopsych.2010.04.007},
  abstract    = {The N-methyl-D-aspartate receptor (NMDAR) hypofunction model of schizophrenia is based on the ability of NMDAR antagonists to produce many symptoms of the disease. Recent work in rats shows that NMDAR antagonist works synergistically with dopamine to produce delta frequency bursting in the thalamus. This finding, together with other results in the literature, suggests a mechanism for the sudden onset of schizophrenia. Among the thalamic nuclei most activated by NMDAR antagonist is the nucleus reuniens. This nucleus excites the cornu ammonis area 1 (CA1) region of the hippocampus. Experiments indicate that such activation can lead to excitation of dopaminergic cells of the ventral tegmental area by a polysynaptic pathway. The resulting elevation of dopamine in the thalamus will enhance thalamic bursting, thereby creating a loop with the potential for positive feedback. We show through computer simulations that in individuals with susceptibility to schizophrenia (e.g., because of partially compromised NMDAR function), an event that stimulates the dopamine system, such as stress, can cause the system to reach the threshold for thalamic bursting. When this occurs, positive feedback in the loop will cause all components to become highly active and to remain active after the triggering stimulus is removed. This is a physiologically specific hypothesis for the sudden and lasting transition that underlies the psychotic break in schizophrenia. Furthermore, the model provides an explanation for the observed selective activation of the CA1 hippocampal region in schizophrenia. The model also predicts an increase of basal activity in the dopamine system and thalamus; the relevant evidence is reviewed.},
  file        = {Lisman2010thalamo.pdf:Lisman2010thalamo.pdf:PDF;:Lisman2010thalamo_supplementary.pdf:PDF;Lisman2010thalamo_supplementary.pdf:Lisman2010thalamo_supplementary.pdf:PDF;Lisman2010thalamo.txt:Lisman2010thalamo.txt:Text},
  institution = {Department of Biology, Brandeis University, Waltham, Massachusetts 02454, USA.},
  journal     = {Biol Psychiatry},
  keywords    = {Animals; Dopamine, metabolism; Excitatory Amino Acid Antagonists, pharmacology; Feedback, Physiological; Hippocampus, drug effects/metabolism/physiopathology; Humans; Neural Pathways, drug effects/metabolism/physiopathology; Rats; Receptors, N-Methyl-D-Aspartate, antagonists /&/ inhibitors/metabolism; Schizophrenia, chemically induced/pathology; Thalamus, drug effects/metabolism/physiopathology; Ventral Tegmental Area, drug effects/metabolism/physiopathology},
  medline-pst = {ppublish},
  owner       = {mohdsh},
  pii         = {S0006-3223(10)00352-5},
  pmc         = {PMC3507433},
  pmid        = {20553749},
  timestamp   = {2016.04.01},
}

@Article{Lisman2005Recall,
  author      = {Lisman, John E. and Talamini, Lucia M. and Raffone, Antonino},
  title       = {Recall of memory sequences by interaction of the dentate and CA3: a revised model of the phase precession.},
  year        = {2005},
