@Article{Wouterlood1995Parvalbumin,
  author      = {Wouterlood, F. G. and H{\"{a}}rtig, W. and Br{\"{u}}ckner, G. and Witter, M. P.},
  title       = {Parvalbumin-immunoreactive neurons in the entorhinal cortex of the rat: localization, morphology, connectivity and ultrastructure.},
  year        = {1995},
